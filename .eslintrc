{
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest/globals": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parser": "@babel/eslint-parser",
  "plugins": ["babel", "prettier", "jest"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "warn"
  }
}
